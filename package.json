{
  "name": "jest-runner-prettier",
  "version": "0.0.0-development",
  "description": "A prettier runner for Jest",
  "main": "src/index.js",
  "repository": "https://github.com/keplersj/jest-runner-prettier",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "text"
    ],
    "projects": [
      "jest-prettier.config.js",
      "jest-test.config.js"
    ]
  },
  "lint-staged": {
    "*": "jest --bail --findRelatedTests"
  },
  "release": {
    "verifyConditions": "@semantic-release/github"
  },
  "dependencies": {
    "cli-highlight": "^2.0.0",
    "create-jest-runner": "0.4.0",
    "jest-diff": "^22.0.0"
  },
  "peerDependencies": {
    "prettier": ">= 1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "6.2.0",
    "@commitlint/config-conventional": "6.1.3",
    "husky": "0.14.3",
    "jest": "22.4.4",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2",
    "semantic-release": "11.2.0"
  }
}
